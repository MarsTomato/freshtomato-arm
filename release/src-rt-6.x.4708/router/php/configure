if test "$PHP_BZ2" != "no"; then
  if test -r $PHP_BZ2/include/bzlib.h; then
    BZIP_DIR=$PHP_BZ2
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for BZip2 in default path" >&5
printf %s "checking for BZip2 in default path... " >&6; }
    for i in /usr/local /usr; do
      if test -r $i/include/bzlib.h; then
        BZIP_DIR=$i
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: found in $i" >&5
printf "%s\n" "found in $i" >&6; }
        break
      fi
    done
  fi

  if test -z "$BZIP_DIR"; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: not found" >&5
printf "%s\n" "not found" >&6; }
    as_fn_error $? "Please reinstall the BZip2 distribution" "$LINENO" 5
