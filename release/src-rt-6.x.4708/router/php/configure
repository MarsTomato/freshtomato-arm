        *.s) ac_comp="$b_c_pre $ac_inc $b_c_meta -DZEND_ENABLE_STATIC_TSRMLS_CACHE=1 -c $ac_srcdir$ac_src -o $ac_bdir$ac_obj.$b_lo $b_c_post" ;;
